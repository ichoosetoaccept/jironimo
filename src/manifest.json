{
  "manifest_version": 3,
  "name": "Jironimo",
  "description": "Copy Jira session cookie for CLI authentication",
  "version": "0.1.0",
  "permissions": ["cookies", "activeTab"],
  "host_permissions": ["*://*/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.svg",
      "48": "icons/icon48.svg",
      "128": "icons/icon128.svg"
    }
  },
  "icons": {
    "16": "icons/icon16.svg",
    "48": "icons/icon48.svg",
    "128": "icons/icon128.svg"
  }
}
